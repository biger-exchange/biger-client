plugins {
    id 'java'
}

sourceCompatibility = 1.8
targetCompatibility = 1.8

repositories {
    mavenLocal()
    maven {url 'https://biger-exchange.github.io/biger-client/'}
    mavenCentral()
}

dependencies {
    compile 'com.biger:biger-client-core:2.0'
    compile 'com.biger:biger-client-httpops-httpurlconnection:2.0'
}
